    "@babel/preset-typescript": "^7.21.0",
    "@testing-library/react-hooks": "^8.0.1",
    "@types/jest": "^29.5.0",
    "babel-jest": "^29.5.0",
    "jest": "^29.5.0",
    "react": "^18.2.0",
    "react-test-renderer": "^18.2.0",
    "ts-jest": "^29.0.5",
    "tsdx": "^0.14.1",
    "@enouvo/eslint-config-react": "^1.5.0",
    "@types/node": "^18.11.9",
    "@types/react": "^18.0.25",
    "@typescript-eslint/parser": "^5.45.0",
    "eslint": "^8.28.0",
